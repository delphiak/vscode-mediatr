{
	"name": "mediatr",
	"displayName": "MediatR",
	"publisher": "delphiak",
	"description": "Provides useful extensions for work with MediatR in C#",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:csharp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "MediatR",
			"properties": {
				"mediatr.goToImplementationTrigger": {
					"type": "string",					
					"default": "(Command|Event)",
					"description": "Implementation provider will only be triggered if item matches this regular expression"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
